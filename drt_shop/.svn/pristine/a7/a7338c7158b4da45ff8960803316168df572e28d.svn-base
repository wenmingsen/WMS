        <div class="look_title" id="logindiv" style="display:none">
            <i></i>
           <span id="info"></span>
      </div>
        <div style="height:80px;"></div>
    </div>
    <script type="text/javascript" src="../../js/jquery.min.js"></script>
    <script type="text/javascript" src="../../js/layer/layer.js"></script>
    <script type="text/javascript" src="../../common/common.js"></script>
    <script src="../../js/mui/mui.min.js"></script>
    
    <script type="text/javascript">
    $(function(){
    	init();
    })
     
    var ctx=getRootPath();
    function init(){
    	$.get(getRootPath()+'/ins/mall/me/getMeInfo', function(result){
    		if(result.state=="-1"){
				$("#logindiv").show();
				$("#info").html(result.message);
				$("#meInfo").hide();
				return;
			}else{
				if(result.data){
	    			var imgUrl = result.data.imgUrl || '';
	    			var nickname = result.data.nickname || '';
	    			if(imgUrl != null && imgUrl != undefined && imgUrl != '' && imgUrl.trim() != ''){
	    				$("#imgUrl").attr("src", imgUrl);
	    			}
	    			if(nickname){
	    				$("#nickname").text(nickname);
	    			}
	    		}
			}
    		
    	})
    }
    mui(".mui-bar-nav").on('tap','.mui-icon-left-nav',function(){
		location.href="main.html";
	})
    </script>
