.shopName{
    border-bottom: 1px solid #dedede;
    font-size: 14px;
    padding: 10px 10px 0px 10px;
    height: 30px;
}
.main_panel{
	background:#ffffff;
	height:auto;
}
.main_panel_info{
	border:1px solid #dedede;
	padding:10px;
}
.panel_title{
	background-color:#ffffff;
}
.panel_title input{
    border:0px solid;
}
.panel_title input{
	border:none;
	outline:medium;
	float:right;
	text-align:right;
	padding-right:1px;
	background-color:#ffffff;
}
#colorChange{
	height:5px;
	background:repeating-linear-gradient(135deg,#58a 0,#58a 35px,#FFFFFF 35px,#FFFFFF 50px,#FF6666 50px,#FF6666 85px,#FFFFFF 85px,#FFFFFF 100px);
}

#orderState{
      float: right;
      color: #ff5001;
}
.totalPoint{
      float: right;
      color: #ff5001;
}



