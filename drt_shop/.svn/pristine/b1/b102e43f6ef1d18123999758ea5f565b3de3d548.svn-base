#ensure{
    border: 1px solid;
    padding: 2% 35%;
    width: 35;
    text-align: center;
    color: white;
    background-color: #184f84;
}
select{
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	border:0px solid;
	font-size:16px;
	outline:medium;
	background-color:white;
}
select::-ms-expand{
	display:none;
}    
input{
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	border:0px solid;
	font-size:16px;
	outline:medium;
	background-color:white;
}
input[type="search"]{-webkit-appearance:none;} 

.mui-bar-nav~.mui-content{
	padding-top: 15px;
}
.panelName,.panelContent{
	float:left;
	text-align:center;
}

